{
  "name": "dashboard-ui",
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "dashboard-ui:build",
        "dev": true
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "options": {"outputPath": "dist/apps/dashboard-ui"},
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo linting..."
      }
    }
  }
}
